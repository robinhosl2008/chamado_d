/* CSS Document */

/****************** configuraÃ§Ãµes gerais *******************/
#menuGeral{
	height:70px;
	position:relative;
	font-family: "Century Gothic", Tahoma, Verdana, Arial;
	background: url(/images/bgMenu.jpg) top no-repeat;
	margin-bottom:1px;
	padding-top:10px;
	}
/****************** configuraÃ§Ãµes gerais fim *******************/


/****************** configuraÃ§Ãµes menu *******************/
#menuGeral li{
	margin-bottom:5px;
	list-style:none;
	float:left;
	}
.menuPrincipal{
	padding-bottom:30px;
	}
#menuGeral ul{
	margin:0px;
	padding:0px;
	}
.menu {
	background: url(/images/separadorMenu.gif) right no-repeat;
	padding: 0px 10px 0px 10px;
	font-size:14px;
	font-weight:bold;
	}
/****************** configuraÃ§Ãµes menu fim*******************/


/****************** configuraÃ§Ãµes submenu *******************/
#menuGeral .submenu {
	margin-top: 12px;
	background-image:url(/images/sombraColuna.png);
	background-position:top;
	background-repeat:no-repeat;
	min-height:15px;
	padding-top:10px;
	padding-bottom:5px;
	width:100%;
	position:absolute;
	left:0px;
	font-weight:bold;
	}
#menuGeral .submenu  a{ color:#FFF;}
.submenu li {
	padding: 0px 10px 0px 10px;
}
#menu div.on {
	display:block;
}
#menu div.off {
	display:none;
}
#menu li.on {
	background-color:#9C0;
}
#menu li.off {
	background-color:#960;
}
/****************** configuraÃ§Ãµes submenu fim *******************/


/****************** configuraÃ§Ãµes menuinterno *******************/
#menuInterno {
	background-color:#eee
}
#menuInterno{
	margin:0px;
	padding:0px;
}
#menuInterno li{
	border-bottom:1px solid #CCC;
	padding:10px 5px;
	margin:0px;
	background: url(/images/bgMenu.jpg) no-repeat; 
	list-style: none;
}
#menuInterno li:hover{
	background: url(/images/bgMenuHover.jpg) no-repeat;
}
/****************** configuraÃ§Ãµes menuinterno fim *******************/


