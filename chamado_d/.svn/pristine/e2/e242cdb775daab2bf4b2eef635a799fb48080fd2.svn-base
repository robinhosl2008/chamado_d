@charset "utf-8";
/* CSS Document */

/* Definições
--------------------------------------------------------------------------------------------------------------------------------------------------------------------*/
body{
	font-family: Tahoma, Arial, Verdana, sans-serif;
	background: url(/images/sombraGeral.png) top no-repeat ;
	font-size:12px;
	color:#333;
	margin:0px;
}
a { color:#333}
a:hover { color:#21668F}
a, a:hover, a:active, a:visited {
	text-decoration: none;
	
}
a {outline: 0;}
* { outline:none;}
h1, h2, h3, h4, h5, h6, p{margin:0px 0px 10px 0px;}
h1{font-size:15px;
	font-family: "Century Gothic", Arial, Verdana,sans-serif; }
