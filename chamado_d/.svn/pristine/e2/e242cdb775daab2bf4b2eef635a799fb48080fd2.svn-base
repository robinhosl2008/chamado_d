/*Erro de validação*/
form label.error{  
 	text-align: right; 
 	width: 100%; 
 } 
 form label.error { 
    color: red; 
    font-style: italic; 
    font-size: 0.9em; 
    margin-top: -10px; 
 } 
 input { 
     border: 1px solid black; 
 } 
 input.checkbox { 
     border: medium none; 
 } 
 input:focus { 
      border: 1px dotted black; 
 } 
 input.error { 
     border: 1px dotted red; 
 } 

