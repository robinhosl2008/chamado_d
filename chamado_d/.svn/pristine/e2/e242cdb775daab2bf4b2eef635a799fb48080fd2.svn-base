/* Accordion
--------------------------------------------------------------------------------------------------------------------------------------------------------------------*/
.accordion {
	BORDER-BOTTOM: #c4c4c4 1px solid; WIDTH: auto
}
.accordion H3 {
	BORDER-BOTTOM: medium none; BORDER-LEFT: #c4c4c4 1px solid; PADDING-BOTTOM: 7px; MARGIN: 0px; PADDING-LEFT: 15px; PADDING-RIGHT: 15px; FONT: bold 120%/100% Arial, Helvetica, sans-serif; BACKGROUND: url(http://portal.saude.gov.br/portal/geral/imagens/arrow-square.gif) #e9e7e7 no-repeat right -51px; COLOR: #0099cc; BORDER-TOP: #c4c4c4 1px solid; CURSOR: pointer; BORDER-RIGHT: #c4c4c4 1px solid; PADDING-TOP: 7px
}
.accordion H3:hover {
	BACKGROUND-COLOR: #e3e2e2
}
.accordion H3.active {
	BACKGROUND-POSITION: right 5px
}
.accordion P {
	BORDER-LEFT: #c4c4c4 1px solid; PADDING-BOTTOM: 20px; MARGIN: 0px; PADDING-LEFT: 15px; PADDING-RIGHT: 15px; BACKGROUND: #f7f7f7; BORDER-RIGHT: #c4c4c4 1px solid; PADDING-TOP: 10px
}

